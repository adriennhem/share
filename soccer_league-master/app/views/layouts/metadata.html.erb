<% if @graph %>
  <head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# <%="#{FB_APP_NAMESPACE}"%>: http://ogp.me/ns/fb/<%="#{FB_APP_NAMESPACE}"%>#">
    <meta property="fb:app_id" content="<%="#{FB_SL_APP_ID}"%>" /> 
    <meta property="og:type"   content="<%="#{FB_APP_NAMESPACE}"%>:@graph.graph_type_name" /> 
    <meta property="og:url"    content="<%="#{request.protocol}#{request.host_with_port}#{request.fullpath}" %>" /> 
    <meta property="og:title"  content="<%=@graph.model.try(:name)%>" /> 
    <meta property="og:image"  content="<%="#{request.protocol}#{request.host_with_port}/images/#{@graph.graph_type_name}.jpg" %>" /> 
<% else %>
  <head>
<% end %>
